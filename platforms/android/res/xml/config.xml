<?xml version='1.0' encoding='utf-8'?>
<widget id="com.threedailythings" version="0.0.8" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <icon density="ldpi" src="lib/website/favicon.png" />
    <icon density="mdpi" src="lib/website/favicon.png" />
    <icon density="hdpi" src="lib/website/favicon.png" />
    <icon density="xhdpi" src="lib/website/favicon.png" />
    <name>Three Daily Things</name>
    <description>
        A motivational tool to help you work on the three daily things that will improve your life.
    </description>
    <author email="matthewcasperson@gmail.com" href="http://threedailythings.com">
        Matthew Casperson
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
